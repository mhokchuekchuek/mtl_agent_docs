    classDef execute fill:#dbeafe,stroke:#3b82f6,stroke-width:2px,color:#1e40af
    classDef subagent fill:#cffafe,stroke:#06b6d4,stroke-width:2px,color:#0e7490
    classDef scratch fill:#fce7f3,stroke:#ec4899,stroke-width:2px,color:#be185d
    classDef output fill:#d1fae5,stroke:#10b981,stroke-width:2px,color:#065f46

    class A agent
    class B planning
    class C execute
    class D,E,F subagent
    class G scratch
    class H output
